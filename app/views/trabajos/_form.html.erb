    <%= f.input :i3 %>
    <%= f.input :i4 %>
    <%= f.input :i5 %>
    <%= f.input :i6 %>
    <%= f.input :i7 %>
    <%= f.input :i8 %>
    <%= f.input :i9 %>
    <%= f.input :i10 %>
    <%= f.input :i11 %>
    <%= f.input :i12 %>
    <%= f.input :v1 %>
    <%= f.input :v2 %>
    <%= f.input :v3 %>
    <%= f.input :v4 %>
    <%= f.input :v5 %>
    <%= f.input :v6 %>
    <%= f.input :v7 %>
    <%= f.input :v8 %>
    <%= f.input :v9 %>
    <%= f.input :v10 %>
    <%= f.input :v11 %>
    <%= f.input :v12 %>
    <%= f.input :centro_costo %>
    <%= f.input :supervisor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
